    # Функция для обработки части выражения
    def evaluate_expression(expression):
        operators = []
        values = []
        i = 0

        while i < len(expression):
            if expression[i] in operator_to_func:
                operators.append(expression[i])
                i += 1
            else:
                num = 0
                while i < len(expression) and expression[i] in word_to_num:
                    num += word_to_num[expression[i]]
                    i += 1
                values.append(num)

        for operator in ['умножить', 'плюс', 'минус']:
            while operator in operators:
                index = operators.index(operator)
                result = operator_to_func[operator](values[index], values[index + 1])
                del operators[index]
                values[index] = result
                del values[index + 1]

        return values[0]

    result = evaluate_expression(words)

    # Преобразуем результат в текст
    num_to_word = {v: k for k, v in word_to_num.items()}
    if result in num_to_word:
        return num_to_word[result]
    else:
        return "Некорректный результат"

